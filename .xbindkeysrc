"pactl set-sink-volume 0 +10%"
XF86AudioRaiseVolume

"pactl set-sink-volume 0 -10%"
XF86AudioLowerVolume

"light -A 10"
XF86MonBrightnessUp

"light -U 10"
XF86MonBrightnessDown

"clipmenu -i"
Mod4 + c

"clipdel -d .*"
Mod4 + x

