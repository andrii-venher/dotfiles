"$HOME/.bin/display"
Mod4 + F7

"maim -s | xclip -selection clipboard -t image/png"
Print + Release

"maim -i $(xdotool getactivewindow) | xclip -selection clipboard -t image/png"
Shift + Print + Release

"maim -s ~/screenshots/$(date +%s).png"
Mod4 + Print + Release

"maim -i $(xdotool getactivewindow) ~/screenshots/$(date +%s).png"
